torchrun --nproc_per_node=2 train.py\
    --model sparse_token_batch_vit_b_16 --fit-type 'r' --basis Identity --modes 128 --sensors 128 --tokens 32\
    --epochs 100 --batch-size 128 --opt adamw --lr 0.003 --dropout 0.1 --attention-dropout 0.1 --wd 0.3\
    --lr-scheduler cosineannealinglr --lr-warmup-method linear --lr-warmup-epochs 30 --lr-warmup-decay 0.033 --amp\
